node("docker") {
        
        stage 'Checkout'
            git url: "git@github.com:ronovillo/bootcamp.git", branch: "master"
        
            sh "git rev-parse HEAD > .git/commit-id"
            def commit_id = readFile('.git/commit-id').trim()
            println commit_id
    
        stage "build"
            def app = docker.build "test:latest"
    
        stage "publish"
            echo 'Deploy'
}